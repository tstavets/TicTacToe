        android:animation="@animator/hangman_hide_limb_in_noose"
        android:name="body"/>
    <target
        android:animation="@animator/hangman_hide_limb_in_noose"
        android:name="left_leg"/>
    <target
        android:animation="@animator/hangman_hide_limb_in_noose"
