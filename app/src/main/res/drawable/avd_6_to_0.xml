    android:drawable="@drawable/hangman_0">

    <target
        android:animation="@animator/hangman_6_to_0"
        android:name="head"/>
    <target
        android:animation="@animator/hangman_6_to_0"
        android:name="body"/>
    <target
        android:animation="@animator/hangman_6_to_0"
        android:name="left_leg"/>
    <target
        android:animation="@animator/hangman_6_to_0"
        android:name="right_leg"/>
    <target
        android:animation="@animator/hangman_6_to_0"
        android:name="left_arm"/>
    <target
        android:animation="@animator/hangman_6_to_0"
        android:name="right_arm"/>

    <target
